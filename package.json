{
  "name": "ydb-qdrant",
  "version": "1.0.0",
  "main": "dist/Api.js",
  "types": "dist/Api.d.ts",
  "exports": {
    ".": "./dist/Api.js",
    "./server": "./dist/server.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest",
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "start": "node --experimental-specifier-resolution=node --enable-source-maps dist/index.js",
    "smoke": "npm run build && node --experimental-specifier-resolution=node --enable-source-maps dist/SmokeTest.js",
    "lint": "biome check .",
    "prepublishOnly": "npm test && npm run build"
  },
  "keywords": [
    "ydb",
    "vector-search",
    "approximate-nearest-neighbor",
    "qdrant-compatible",
    "embeddings",
    "nodejs",
    "typescript",
    "express",
    "yql",
    "grpc",
    "yandex-cloud",
    "ann",
    "semantic-search",
    "rag",
    "pino",
    "zod"
  ],
  "author": "",
  "license": "ISC",
  "description": "Qdrant-compatible Node.js/TypeScript API that stores/searches embeddings in YDB using approximate coarse-to-fine vector search (quantized uint8 preselect + float refine).",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.10.0",
    "@grpc/grpc-js": "^1.14.0",
    "@ydbjs/api": "^6.0.4",
    "@ydbjs/core": "^6.0.4",
    "@ydbjs/query": "^6.0.4",
    "@ydbjs/value": "^6.0.4",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "nice-grpc": "^2.1.13",
    "pino": "^10.1.0",
    "ydb-sdk": "^5.11.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.7",
    "@types/express": "^5.0.3",
    "@types/node": "^24.9.1",
    "docsify-cli": "^4.4.4",
    "node-plantuml-latest": "^2.4.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  }
}